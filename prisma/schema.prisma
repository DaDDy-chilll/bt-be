

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model m_users {
  id         Int      @id @default(autoincrement())
  name       String
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  del_flg    Boolean?
}
